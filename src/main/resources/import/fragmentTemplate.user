
<user script="classpath://import/import-user_v2-6.crn" username="fragmentTemplate">
  <default-user>demo</default-user>
  <profile name="HTML browser profile"> 
    <description>A sample profile for common web browsers</description>  
    <structure name="DLM Tabs and columns">
      <!--Parameters for structure transform-->
      <parameter> 
        <name>activeTab</name>  
        <value>1</value> 
      </parameter>
    </structure>  
    <theme name="DLM XHTML">
      <!--Parameters for theme transform-->
      <parameter> 
        <name>skin</name>  
        <value>spring</value> 
      </parameter>
    </theme> 
  </profile>
</user>
