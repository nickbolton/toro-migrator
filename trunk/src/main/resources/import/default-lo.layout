
<layout script="classpath://import/import-layout_v2-6.crn" username="default-lo">
  <root name="Root folder" hidden="N" immutable="N" unremovable="Y">
    <header name="Header folder" hidden="N" immutable="Y" unremovable="Y">
      <channel fname="header"/>
      <channel fname="portal/login/general"/>
      <channel fname="session-locales-selector"/>
    </header>
    <footer name="Footer folder" hidden="N" immutable="N" unremovable="N">
      <channel fname="footer"/>
    </footer>
    <tab name="Message Center" hidden="N" immutable="N" unremovable="N">
      <column name="Column" hidden="N" immutable="N" unremovable="N">
        <structure-attribute type="folder">
          <name>width</name>
          <value>50%</value>
        </structure-attribute>
        <channel fname="Campus Announcements"/>
        <channel fname="Campus News"/>
      </column>
      <column name="Column" hidden="N" immutable="N" unremovable="N">
        <structure-attribute type="folder">
          <name>width</name>
          <value>50%</value>
        </structure-attribute>
        <channel fname="MessagingPortlet"/>
        <channel fname="Classifieds"/>
      </column>
    </tab>
    <tab name="Collaboration" hidden="N" immutable="N" unremovable="N">
      <column name="Column" hidden="N" immutable="N" unremovable="N">
        <structure-attribute type="folder">
          <name>width</name>
          <value>50%</value>
        </structure-attribute>
        <channel fname="Survey"/>
        <channel fname="BlojsomGatewayPortlet"/>
      </column>
      <column name="Column" hidden="N" immutable="N" unremovable="N">
        <structure-attribute type="folder">
          <name>width</name>
          <value>50%</value>
        </structure-attribute>
        <channel fname="Poll"/>
      </column>
    </tab>
    <tab name="Personal" hidden="N" immutable="N" unremovable="N">
      <column name="Column" hidden="N" immutable="N" unremovable="N">
        <structure-attribute type="folder">
          <name>width</name>
          <value>50%</value>
        </structure-attribute>
        <channel fname="Calendar (IBS)"/>
        <channel fname="Bookmarks"/>
      </column>
      <column name="Column" hidden="N" immutable="N" unremovable="N">
        <structure-attribute type="folder">
          <name>width</name>
          <value>50%</value>
        </structure-attribute>
        <channel fname="AB"/>
        <channel fname="My Notes"/>
      </column>
    </tab>
    <tab name="Briefcase" hidden="N" immutable="N" unremovable="N">
      <column name="Column" hidden="N" immutable="N" unremovable="N">
        <channel fname="Briefcase Portlet"/>
      </column>
    </tab>
  </root>
</layout>
